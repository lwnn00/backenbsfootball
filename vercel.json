{
  "functions": {
    "server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
  "version": 2,
  "builds": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
  "routes": [
    {
      "source": "/api/(.*)",
      "headers": [
